# Local Development Environment Variables

# API Keys
API_KEY=local_development_key_123
FIREBASE_API_KEY=firebase_local_dev_key_456
OPENAI_API_KEY=openai_local_dev_key_789

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=kimiyomi_local_db
DB_USER=local_user
DB_PASSWORD=local_password

# Authentication
AUTH_SECRET=local_auth_secret_key
JWT_SECRET=local_jwt_secret_key

# Services
FIREBASE_PROJECT_ID=kimiyomi-local
FIREBASE_APP_ID=1:123456789:web:abcdef
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_STORAGE_BUCKET=kimiyomi-local.appspot.com

# External Services
STRIPE_PUBLIC_KEY=pk_test_sample
STRIPE_SECRET_KEY=sk_test_sample

# Feature Flags
ENABLE_CHAT_FEATURE=true
ENABLE_DIAGNOSIS_FEATURE=true
ENABLE_PREMIUM_FEATURES=true

# Logging
LOG_LEVEL=debug
ENABLE_DEBUG_MODE=true

# Cache
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# URLs
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:8000
WEBSOCKET_URL=ws://localhost:8080